    "#Function to convert Size column to integer, in kilobytes\n",
    "\n",
    "def convert_bytes(size):\n",
    "    x = 0\n",
    "    if 'M' in size:\n",
    "        x = size[:-1]\n",
    "        x = float(x) * 1000\n",
    "        return(x)\n",
    "    elif 'k' in size:\n",
    "        x = size[:-1]\n",
    "        return float(x)\n",
    "    else:\n",
    "        return float(x)\n",
    "\n",
    "joined_data[\"Size\"] = joined_data[\"Size\"].apply(convert_bytes) \n",
    "joined_data"
