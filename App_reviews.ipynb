   "source": [
    "#Function to convert size column to integer, in megabytes\n",
    "def convert_bytes(size):\n",
    "    if 'M' in size:\n",
    "        x = size[:-1]\n",
    "        x = float(x) * 1000\n",
    "        return(float(x))\n",
    "    elif 'k' in size:\n",
    "        return size[:-1]\n",
    "    else:\n",
    "        return 0\n",
    "\n",
    " \n",
    "app[\"Size\"] = app[\"Size\"].map(convert_bytes)\n",
    "app.dtypes"
   ]
